                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    s.SalesPersonID AS SourceEmployeeID,
    s.CustomerID AS SourceCustomerID,
    CAST(s.SalesDate AS DATE) AS SourceDate,
    p.CategoryID AS SourceCategoryID,
    cust.CityID AS SourceCityID,
    s.ProductID AS SourceProductID,
    SUM(s.TotalPrice) AS TotalSpent,
    COUNT(DISTINCT s.SalesID) AS NumberOfTransactions,
    GETDATE() AS ExtractDate,
    'GrocerySales' AS SourceSystem
FROM 
    Sales s
    JOIN Customers cust ON s.CustomerID = cust.CustomerID
    JOIN Products p ON s.ProductID = p.ProductID
WHERE 
    s.SalesDate BETWEEN ? AND ?
    AND s.Quantity &gt; 0  -- Exclude returns/voids
GROUP BY
    s.SalesPersonID,
    s.CustomerID,
    CAST(s.SalesDate AS DATE),
    p.CategoryID,
    cust.CityID,
    s.ProductID</property>
